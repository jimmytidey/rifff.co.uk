package {		import flash.events.*;	import flash.utils.*;	import flash.display.*; 	import flash.text.*; 	import flash.MovieClip; 		public class LoadContext extends MovieClip {				function debug() { 			var user_name:String = root.loaderInfo.parameters.project_name;			var debug_state; 			if (user_name == null) {				trace("debug");				debug_state = "true"; 			}			else {debug_state="false"; }						return debug_state; 		}				function userName() { 			var user_name:String = root.loaderInfo.parameters.user_name;			if (user_name == null) {				user_name = "tom"; 			}			return user_name; 		}				function projectName() { 					var project_name:String;			if (root.loaderInfo.parameters.project_name) {				project_name = root.loaderInfo.parameters.project_name;			}			else {				project_name = "Unstable Debug Version"; 			}			return project_name; 		}	}}